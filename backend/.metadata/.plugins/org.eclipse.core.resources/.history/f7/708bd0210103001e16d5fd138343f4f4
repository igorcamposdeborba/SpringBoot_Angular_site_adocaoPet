package com.adocao.pet;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.boot.web.servlet.ServletComponentScan;

@SpringBootApplication
@ServletComponentScan // listener para a configuração do CORS origin (se estiver executando o Spring Boot e Angular a partir de portas diferentes)
public class AdocaoPetApplication {

	public static void main(String[] args) {
		SpringApplication.run(AdocaoPetApplication.class, args);
	}

}
